def insertion_sort(arr):
  
    for i in range(1, len(arr)):

        key = arr[i]

       
        j = i-1
        while j >= 0 and key < arr[j] :
                arr[j + 1] = arr[j]
                j -= 1
        arr[j + 1] = key
 
    return arr

l=[2,10,5,8,15,21]
print(insertion_sort(l))        